mb-2\" style={{display: item.remove ? 'none': 'flex'}}>\n                    <Col>\n                        <Form.Control\n                            name={'name_' + item.unique}\n                            value={item.name}\n                            onChange={e => change('name', e.target.value, item.unique)}\n                            placeholder=\"–ù–∞–∑–≤–∞–Ω–∏–µ...\"\n                            size=\"sm\"\n                        />\n                    </Col>\n                    <Col>\n                        <Form.Control\n                            name={'value_' + item.unique}\n                            value={item.value}\n                            onChange={e => change('value', e.target.value, item.unique)}\n                            placeholder=\"–ó–Ω–∞—á–µ–Ω–∏–µ...\"\n                            size=\"sm\"\n                        />\n                    </Col>\n                    <Col>\n                        <Button onClick={() => remove(item.unique)} size=\"sm\" variant=\"outline-danger\">\n                            –£–¥–∞–ª–∏—Ç—å\n                        </Button>\n                        {item.change && ' *'}\n                    </Col>\n                </Row>\n            )}\n        </>\n    )\n}\n\nexport default UpdateProperties",{"ruleId":"290","replacedBy":"291"},{"ruleId":"292","replacedBy":"293"},{"ruleId":"294","severity":1,"message":"295","line":4,"column":21,"nodeType":"296","messageId":"297","endLine":4,"endColumn":24},{"ruleId":"294","severity":1,"message":"298","line":4,"column":26,"nodeType":"296","messageId":"297","endLine":4,"endColumn":29},{"ruleId":"294","severity":1,"message":"299","line":4,"column":31,"nodeType":"296","messageId":"297","endLine":4,"endColumn":38},{"ruleId":"300","severity":1,"message":"301","line":33,"column":8,"nodeType":"302","endLine":33,"endColumn":10,"suggestions":"303"},{"ruleId":"294","severity":1,"message":"304","line":2,"column":10,"nodeType":"296","messageId":"297","endLine":2,"end